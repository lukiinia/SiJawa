<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/android_lar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="687dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="27dp"
        android:src="@drawable/persegipanjang" />

    <ImageButton
        android:id="@+id/imag"
        android:layout_width="55dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="351dp"
        android:layout_marginBottom="813dp"
        android:backgroundTint="#ffff"
        android:src="@drawable/vectorback" />

    <Button
        android:id="@+id/button4"
        android:layout_width="262dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="471dp"
        android:backgroundTint="#C33737"
        android:text="Keluar" />

    <Button
        android:id="@+id/button5"
        android:layout_width="267dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="532dp"
        android:backgroundTint="#D9D9D9"
        android:text="batal"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="302dp"
        android:layout_height="103dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="283dp"
        android:layout_marginEnd="35dp"
        android:text="Anda yakin ingin keluar?"
        android:textColor="#A44E0C"
        android:textSize="28sp"
        android:textStyle="bold" />


</RelativeLayout>